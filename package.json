{
  "name": "senior-backend-test-risevest",
  "version": "1.0.0",
  "description": "A senior backend test for Risevest.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "docker:sh": "docker compose up -d && docker exec -it senior-backend-test-risevest-api-1 sh",
    "docker:start:sh": "docker compose up -d && docker exec -it senior-backend-test-risevest-api-1 sh",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:dev": "nodemon --config tooling/nodemon.json --inspect",
    "test": "jest -c tooling/jest.config.ts",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": "George Ijidola",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/redis": "^6.1.1",
    "@fastify/sensible": "^5.5.0",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.29",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^10.3.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  }
}
