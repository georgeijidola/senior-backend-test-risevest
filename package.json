{
  "name": "senior-backend-test-risevest",
  "version": "1.0.0",
  "description": "A senior backend test for Risevest.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "db:setup": "ts-node db/setup.ts || node db/setup.js",
    "db:migrate:do": "ts-node db/index.ts do || node db/index.js do",
    "db:migrate:undo": "ts-node db/index.ts undo || node db/index.js undo",
    "docker:sh": "docker exec -it senior-backend-test-risevest-api-1 sh",
    "docker:start:sh": "docker compose up -d && docker exec -it senior-backend-test-risevest-api-1 sh",
    "start": "node -r ts-node/register ./src/index.ts",
    "start:dev": "nodemon --config tooling/nodemon.json --inspect",
    "test": "jest -c tooling/jest.config.ts",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": "George Ijidola",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "postgrator": "^7.2.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1",
    "ts-node": "^10.9.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.29",
    "@types/pg": "^8.11.3",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  }
}
